{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "pp_file",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "PP_files",
						"type": "DatasetReference",
						"parameters": {
							"p_container": {
								"value": "@pipeline().parameters.pp_container",
								"type": "Expression"
							},
							"p_directory": {
								"value": "@pipeline().parameters.pp_input",
								"type": "Expression"
							},
							"p_filename": {
								"value": "@pipeline().parameters.pp_filename",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"pp_container": {
				"type": "string",
				"defaultValue": "jagdatacontainer1"
			},
			"pp_input": {
				"type": "string",
				"defaultValue": "Project1/input"
			},
			"pp_filename": {
				"type": "string",
				"defaultValue": "SalesLTProduct.txt"
			},
			"pp_output": {
				"type": "string",
				"defaultValue": "Project1/output"
			}
		},
		"folder": {
			"name": "Git_link"
		},
		"annotations": []
	}
}